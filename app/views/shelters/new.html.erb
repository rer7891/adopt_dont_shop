<form action='/shelters' method='post'>
  <input type='hidden' name='authenticity_token' value='%= form_authenticity_token %>'
  <p> Enter a new Shelter: </p>
  <p> Shelter Name: <input type='text' name='shelter[name]'/></p>
  <p> Address: <input type='text' name='shelter[address]'></p>
  <p> City: <input type='text' name='shelter[city]'> </p>
  <p> Zip:  <input type='text' name='shelter[zip]'> </p>
  <input type='submit'/>
</form>
