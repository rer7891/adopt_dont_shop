<%= form_tag "/shelters/#{@shelter.id}", method: :patch do %>
  <%= label_tag :name %>
  <%= text_field_tag(:shelter, @shelter.name) %>
  <%= label_tag :address %>
  <%= text_field_tag(:shelter, @shelter.address) %>
  <%= label_tag :city %>
  <%= text_field_tag(:shelter, @shelter.city) %></br>
  <%= label_tag :state %>
  <%= text_field_tag(:shelter, @shelter.state) %></br>
  <%= label_tag :zip %>
  <%= text_field_tag(:shelter, @shelter.zip) %></br>
  <%= submit_tag 'Update Shelter' %>
<% end %>
