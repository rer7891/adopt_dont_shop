
<%= form_tag "/shelters/#{params[:shelter_id]}/pets" do %>
  <%= label_tag :image %>
  <%= text_field_tag :image %>
  <%= label_tag :name %>
  <%= text_field_tag :name %>
  <%= label_tag :description %>
  <%= text_field_tag :description %></br>
  <%= label_tag :age %>
  <%= text_field_tag :age %></br>
  <%= label_tag :sex %>
  <%= text_field_tag :sex %></br>
  <%= submit_tag 'Create Pet' %>
<% end %>
